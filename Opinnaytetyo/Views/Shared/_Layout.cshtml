<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Opinnäyte</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("head", required: false)
    <link href="tyyli.css" rel="stylesheet" />


</head>
<body>
    <div class="container">
        <!-- Navigation -->
        <div class="row">
            <nav class="navbar navbar-inverse" role="navigation">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand visible-xs-inline-block nav-logo" href="/"><img src="/images/evil2.jpg" class="img-responsive" alt=""></a>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse navbar-ex1-collapse">
                        <ul class="nav navbar-nav js-nav-add-active-class">

                            <!-- <li><a href="otayhteytta.html">Etusivu</a></li>-->
                            <li class="active"><a href="/home/Index">Home</a></li>
                            <li><a href="/Oppilas">Opiskelijat</a></li>
                            <li><a href="/Account/KayttajaListaus">Käyttäjätiedot</a></li>
                            <!---    <li><a href="/Home/Youtube/KayttajaListaus">Tutorialit</a></li> -->
                            <li><a href="/Account/Register">Rekisteröidy</a></li>
                            <li class="visible-xs-block"><a href="#">About Us</a></li>
                            <li class="visible-xs-block"><a href="#">Careers</a></li>
                            <li class="visible-xs-block"><a href="#">Press Release</a></li>
                            <li class="visible-xs-block"><a href="#">Support</a></li>


                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Tutorialit <b class="caret"></b></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="/Home/Youtube">PHP</a></li>
                                    <li><a href="#">Careers</a></li>
                                    <li><a href="#">Press Release</a></li>
                                </ul>
                            </li>

                        </ul>
                        @ViewBag.Date
                        <ul class="nav navbar-nav navbar-right hidden-xs">
                            <a type="button" class="navbar-btn btn btn-gradient-blue" am-latosans="bold" href="#">Support</a>
                        </ul>

                    </div><!-- /.navbar-collapse -->
                    <div class="jumbotron banner" style="text-align:center; min-height: 195px;"></div>
                </div>

                <div class="container">
                   

                    @RenderBody()
                    <hr />
                </div>
                <div class="col-md-12" style="min-height: 40px; text-align: center; padding: 10px;"> Web Design  &amp; Kimi</div>
            </nav>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
